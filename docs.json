{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Parsecular",
  "colors": {
    "primary": "#6366F1",
    "light": "#818CF8",
    "dark": "#4F46E5"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "API Reference",
        "groups": [
          {
