            ]
          },
          {
            "group": "Orders",
            "pages": [
              "api/create-order",
              "api/list-orders",
              "api/get-order",
              "api/cancel-order"
            ]
          },
          {
            "group": "Account",
            "pages": [
              "api/ping",
              "api/list-positions",
              "api/get-balance"
            ]
          }
